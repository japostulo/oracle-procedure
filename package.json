{
  "name": "nest-oracle-procedure",
  "version": "1.1.0",
  "description": "Pacote responsável por padronizar a chamada de procedure com o nest e o oracledb",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc -p . -w",
    "start": "nodemon src/index.js",
    "serve": "nodemon src/index.ts",
    "test": "jest",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/japostulo/oracledb-procedure.git"
  },
  "keywords": [
    "procedure",
    "oracledb"
  ],
  "author": "João Apostulo Neto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/japostulo/oracledb-procedure/issues"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ]
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/japostulo/oracledb-procedure#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "oracledb": "^5.5.0",
    "typeorm": "^0.3.12"
  }
}
